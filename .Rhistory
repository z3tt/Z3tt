data(mite.env)
data(mite)
data(mite.xy)
mite.xy
# create nb object (spatial neighbourhoods)
nbnear.mite<-dnearneigh(as.matrix(mite.xy), 0, 1.5) # use truncation above distance of 1.5
nbnear.mite
